	<queryString>
		<![CDATA[SELECT
     factura.id_factura AS factura_id_factura,
     factura.fecha_emision AS factura_fecha_emision,
     factura.subtotal AS factura_subtotal,
     factura.impuesto AS factura_impuesto,
     factura.total AS factura_total,
     factura.id_cliente AS factura_id_cliente,
     factura.id_empleado AS factura_id_empleado,
     detalle_factura_consulta.precio_consulta AS detalle_factura_consulta_precio_consulta,
     detalle_factura_consulta.diagnostico AS detalle_factura_consulta_diagnostico,
     detalle_factura_consulta.fecha_consulta AS detalle_factura_consulta_fecha_consulta,
     detalle_factura_consulta.nombre_mascota AS detalle_factura_consulta_nombre_mascota,
     detalle_factura_medicamento.subtotal AS detalle_factura_medicamento_subtotal,
     detalle_factura_medicamento.cantidad AS detalle_factura_medicamento_cantidad,
     detalle_factura_medicamento.precio_unitario AS detalle_factura_medicamento_precio_unitario,
     detalle_factura_medicamento.id_medicamento AS detalle_factura_medicamento_id_medicamento
FROM
     factura factura INNER JOIN detalle_factura_consulta detalle_factura_consulta ON factura.id_factura = detalle_factura_consulta.id_factura
     INNER JOIN detalle_factura_medicamento detalle_factura_medicamento ON factura.id_factura = detalle_factura_medicamento.id_factura
WHERE factura.id_factura = $P!{id_factura}]]>
	</queryString>
